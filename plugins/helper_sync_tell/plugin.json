{
    "name": "Ultimate AI Assistant",
    "version": "4.0.0",
    "description": "The most advanced AI assistant for Atlas with meta-cognitive awareness, sophisticated contextual analysis, and human-like reasoning patterns. Represents the pinnacle of AI assistant capabilities based on real assistant experience.",
    "author": "Atlas Team + Ultimate AI Enhancement",
    "main": "ultimate_ai_assistant.py",
    "entry_point": "register",
    "min_python_version": "3.8",
    "max_python_version": "3.13",
    "dependencies": [],
    "platforms_supported": ["linux", "macos"],
    "features": [
        "ultimate_ai_thinking",
        "meta_cognitive_awareness",
        "advanced_contextual_analysis",
        "multi_dimensional_context",
        "adaptive_reasoning",
        "uncertainty_quantification",
        "epistemic_awareness",
        "real_time_adaptation",
        "processing_mode_selection",
        "response_quality_assessment",
        "continuous_learning",
        "human_like_reasoning",
        "sophisticated_tool_integration",
        "cross_domain_knowledge",
        "performance_optimization",
        "graceful_degradation",
        "backward_compatibility"
    ],
    "processing_modes": [
        "lightning",
        "balanced",
        "deep_think",
        "collaborative", 
        "creative"
    ],
    "cognitive_capabilities": [
        "self_monitoring",
        "strategy_adaptation",
        "confidence_calibration",
        "uncertainty_tracking",
        "meta_commentary",
        "knowledge_gap_detection",
        "epistemic_reasoning"
    ],
    "contextual_dimensions": [
        "domain_analysis",
        "intent_classification", 
        "urgency_assessment",
        "complexity_calculation",
        "knowledge_level_inference",
        "interaction_style_adaptation",
        "emotional_tone_analysis"
    ],
    "integration_hooks": ["helper_mode", "post_load", "agent_manager"],
    "configuration": {
        "max_sub_questions": 7,
        "max_processing_time": 60,
        "confidence_threshold": 0.7,
        "auto_mode_selection": true,
        "enable_meta_cognition": true,
        "enable_contextual_analysis": true,
        "enable_uncertainty_tracking": true,
        "enable_adaptive_learning": true,
        "enable_quality_assessment": true,
        "enable_performance_optimization": true,
        "response_refinement": true,
        "session_history_limit": 50
    },
    "advanced_components": {
        "meta_cognitive_engine": true,
        "contextual_analyzer": true,
        "ultimate_reasoning": true,
        "adaptive_learning": true,
        "quality_assessment": true
    },
    "ultimate_ai_features": {
        "meta_cognitive_awareness": "Real-time self-monitoring and adaptation",
        "multi_dimensional_context": "Comprehensive contextual understanding",
        "processing_mode_selection": "Adaptive processing for different scenarios",
        "uncertainty_quantification": "Sophisticated uncertainty tracking and handling",
        "epistemic_reasoning": "Reasoning about knowledge and confidence",
        "human_like_patterns": "Natural thinking patterns based on AI assistant experience",
        "continuous_optimization": "Self-improving performance and adaptation"
    },
    "compatibility": {
        "fallback_mode": true,
        "graceful_degradation": true,
        "hybrid_support": true,
        "backward_compatible": true
    }
}
